{
  "name": "XScout",
  "version": "1.0.0",
  "description": "Scouting application for FIRST Robotics Competition, for Team 612.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --all --out ~\\Desktop --overwrite --ignore=\\.gitignore",
    "package-mac": "electron-packager . --platform=darwin --arch=all --out ~\\Desktop --overwrite --ignore=\\.gitignore",
    "package-win": "electron-packager . --platform=win32 --arch=all --out ~\\Desktop --overwrite --ignore=\\.gitignore",
    "package-linux": "electron-packager . --platform=linux --arch=all --out ~\\Desktop --overwrite --ignore=\\.gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team612/XScout"
  },
  "author": "Team 612",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Team612/XScout/issues"
  },
  "homepage": "https://github.com/Team612/XScout#readme",
  "devDependencies": {
    "electron-packager": "^7.3.0",
    "electron": "^1.2.0"
  }
}
